{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
    "rust": {
      "command": "rust-analyzer",
      "rootPatterns": ["Cargo.toml"],
      "trace.server": "verbose",
      "filetypes": ["rust"]
    }
  }
}
